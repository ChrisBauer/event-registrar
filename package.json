{
  "name": "event-registrar",
  "version": "0.1.0",
  "description": "This library provides a simple and convenient way to register multiple DOM event handlers, including multiple handlers on the same event",
  "main": "index.js",
  "scripts": {
    "test": "node jasmine-runner.js",
    "build": "mkdir -p dist && babel src/event-registrar.js -o dist/event-registrar.js",
    "build-readme": "jsdoc2md src/*.js --template README.tpl > README.md",
    "doc": "jsdoc src/event-registrar.js -d ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisBauer/event-registrar.git"
  },
  "keywords": [
    "event",
    "DOM events",
    "DOM",
    "events",
    "window",
    "onscroll"
  ],
  "author": "Chris Bauer <cbauer@outlook.com> (http://github.com/chrisbauer)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChrisBauer/event-registrar/issues"
  },
  "homepage": "https://github.com/ChrisBauer/event-registrar#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.3"
  }
}
